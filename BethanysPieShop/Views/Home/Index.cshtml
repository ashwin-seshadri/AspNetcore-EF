@model HomeViewModel

<partial name="_Carousal" />

<h2>Pies of the Week</h2>
<h4>Our selection - just for you!</h4>

<div class="row">

    @foreach (var pie in Model.PiesOfTheWeek)
    {
        <div class="col-sm-4 col-lg-4 col-md-4">
            <partial name="_PieCard" model="pie" />
        </div>
    }

</div>
